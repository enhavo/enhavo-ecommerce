{
  "name": "enhavo/enhavo-ecommerce",
  "license": "MIT",
  "description": "enhavo",
  "authors": [
    {
      "name": "Gerhard Seidel",
      "email": "gs@weareindeed.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Enhavo\\": "src/Enhavo"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\": "src/App"
    }
  },
  "require": {
    "php": ">=8.3",
    "doctrine/doctrine-migrations-bundle": "^3.1",
    "enhavo/api-bundle": "dev-master",
    "enhavo/app-bundle": "dev-master",
    "enhavo/block-bundle": "dev-master",
    "enhavo/content-bundle": "dev-master",
    "enhavo/dashboard-bundle": "dev-master",
    "enhavo/doctrine-extension-bundle": "dev-master",
    "enhavo/form-bundle": "dev-master",
    "enhavo/media-bundle": "dev-master",
    "enhavo/metadata": "dev-master",
    "enhavo/navigation-bundle": "dev-master",
    "enhavo/page-bundle": "dev-master",
    "enhavo/routing-bundle": "dev-master",
    "enhavo/setting-bundle": "dev-master",
    "enhavo/revision-bundle": "dev-master",
    "enhavo/resource-bundle": "dev-master",
    "enhavo/type": "dev-master",
    "enhavo/user-bundle": "dev-master",
    "enhavo/vue-form-bundle": "dev-master",
    "symfony/dotenv": "^6.3",
    "symfony/flex": "^1.3.1",
    "symfony/framework-bundle": "^6.3",
    "symfony/http-client": "^6.3",
    "symfony/twig-bundle": "^6.3",
    "symfony/yaml": "^6.3",
    "sylius/addressing": "2.0.*",
    "sylius/addressing-bundle": "2.0.*",
    "sylius/attribute": "2.0.*",
    "sylius/attribute-bundle": "2.0.*",
    "sylius/currency": "2.0.*",
    "sylius/currency-bundle": "2.0.*",
    "sylius/inventory": "2.0.*",
    "sylius/inventory-bundle": "2.0.*",
    "sylius/locale": "2.0.*",
    "sylius/money-bundle": "2.0.*",
    "sylius/order": "2.0.*",
    "sylius/order-bundle": "2.0.*",
    "sylius/payment": "2.0.*",
    "sylius/payment-bundle": "2.0.*",
    "sylius/product": "2.0.*",
    "sylius/product-bundle": "2.0.*",
    "sylius/promotion": "2.0.*",
    "sylius/promotion-bundle": "2.0.*",
    "sylius/resource-bundle": "^1.12",
    "sylius/shipping": "2.0.*",
    "sylius/shipping-bundle": "2.0.*",
    "sylius/taxation": "2.0.*",
    "sylius/taxation-bundle": "2.0.*",
    "tecnickcom/tcpdf": "^6.2"
  },
  "require-dev": {
    "behat/behat": "^3.10",
    "dbrekelmans/bdi": "^0.3.0",
    "overtrue/phplint": "^9.0",
    "phpstan/phpstan": "^0.12.29",
    "phpunit/php-code-coverage": "^9.2",
    "phpunit/phpunit": "^9.6",
    "symfony/debug-pack": "^1.0",
    "symfony/maker-bundle": "^1.0",
    "symfony/panther": "^2.0",
    "symfony/profiler-pack": "^1.0",
    "symfony/test-pack": "^1.0"
  },
  "replace": {
    "enhavo/shop-bundle": "self.version",
    "enhavo/payment-bundle": "self.version",
    "enhavo/sylius-brigde-bundle": "self.version"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "symfony/runtime": true
    }
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    }
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "6.4.*",
      "docker": false
    }
  }
}
